{
  "svcName": "Configure Amazon API Gateway",
  "svcCode": "1001",
  "description": "API Gateway",
  "forms": [
    {
      "name": "HTTP APIs",
      "description": "Select the units, number, and frequency for HTTP API requests",
      "groups":[
        {
          "order":1,
          "fields":[
            {
              "controlType": "textbox",
              "key": "requests",
              "label": "Requests",
              "required": true,
              "order": 1
            }
             
          ]
        },
        {
          "order":2,
          "fields":[
            {
              "controlType": "dropdown",
              "key": "httpapi",
              "label": "HTTP API requests units",
              "options": [
                  { "key": "exactNumber", "value": "ExactNumber" },
                  { "key": "thousands", "value": "Thousands" },
                  { "key": "millions", "value": "Millions" }
               ],
             "order": 1
            },
            {
              "controlType": "dropdown",
              "key": "frequency",
              "label": "Frequency",
              "type": "dropdown",
              "options": [
                { "key": "persecond", "value": "Per Second" },
                { "key": "perminute", "value": "Per Minute" }
              ],
              "order": 2
             
            }
          ]
        },
        {
          "order":3,
          "fields":[
            {
              "controlType": "textbox",
              "key": "averagesize",
              "label": "Average size of each request",
              "type": "textbox",
              "required": true,
              "order": 1
             
            },
            {
              "controlType": "textbox",
              "key": "requestsize",
              "label": "Request Size",
              "type": "textbox",
              "required": true,
              "order": 2
            }
          ]
        }
      ]
    }
  ]
}
      